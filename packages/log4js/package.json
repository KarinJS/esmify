{
  "name": "@karinjs/log4js",
  "version": "1.3.1",
  "description": "Modern TypeScript ESM version of log4js with Node.js 18+ support",
  "keywords": [
    "karinjs",
    "log4js",
    "typescript",
    "esm",
    "logging"
  ],
  "homepage": "https://github.com/KarinJS/log4js",
  "bugs": {
    "url": "https://github.com/KarinJS/log4js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarinJS/log4js.git"
  },
  "license": "MIT",
  "author": "shijin",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsdown",
    "test": "tap --node-arg=\"--import\" --node-arg=\"tsx\" test/*.ts",
    "test:build": "pnpm build && tap test/*.ts",
    "pub": "npm run test && npm publish --access public",
    "sync": "curl -X PUT \"https://registry-direct.npmmirror.com/-/package/@karinjs/log4js/syncs\""
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "date-format": "^4.0.14",
    "debug": "^4.4.3",
    "flatted": "^3.2.7",
    "rfdc": "^1.3.0",
    "streamroller": "^3.1.5",
    "tap": "^16.3.4",
    "tsx": "^4.19.3",
    "typescript": "^5.7.2"
  },
  "source": "a02c81622b6dbb843ca84c22ef83ed5806edbe97"
}